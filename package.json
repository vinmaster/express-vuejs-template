{
  "name": "express-vuejs-template",
  "version": "3.0.0",
  "private": true,
  "description": "Express + Vuejs Template",
  "scripts": {
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "vue-tsc -p client --noEmit && vite build",
    "build-server": "tsc --build tsconfig.server.json",
    "clean": "rm -rf build",
    "mikro-orm": "cross-env MIKRO_ORM_CLI_TS_CONFIG_PATH=tsconfig.server.json mikro-orm",
    "dev": "concurrently \"npm:dev-*\"",
    "dev-client": "vite",
    "dev-server": "cross-env NODE_ENV=development ts-node-dev --project tsconfig.server.json --inspect=4321 --respawn --no-notify ./server/index.ts",
    "heroku-postbuild": "npm run build",
    "server": "node build/index.js",
    "start": "npm run server",
    "start-prod": "cross-env NODE_ENV=production npm start",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.server.json mocha -r ts-node/register \"tests/**/*.spec.ts\"",
    "test-watch": "cross-env NODE_ENV=test TS_NODE_PROJECT=tsconfig.server.json mocha -r ts-node/register --watch --watch-files server, tests/**/*.spec.ts"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.1.3",
    "@mikro-orm/core": "^5.1.3",
    "@mikro-orm/sqlite": "^5.1.3",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.33",
    "@types/passport": "^1.0.7",
    "@types/passport-jwt": "^3.0.6",
    "@types/pino": "^7.0.5",
    "@types/pino-http": "^5.8.1",
    "@types/serve-favicon": "^2.5.3",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^5.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.22.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "ejs": "^3.1.8",
    "express": "^5.0.0-alpha.8",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "ms": "^3.0.0-canary.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "rate-limiter-flexible": "^2.3.7",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "serve-favicon": "^2.5.0",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2",
    "vue": "^3.2.33",
    "vue-router": "^4.0.15",
    "vue-socket.io-extended": "^5.0.0-alpha.5",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@mikro-orm/migrations": "^5.1.3",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/supertest": "^2.0.12",
    "@vitejs/plugin-vue": "^2.3.3",
    "chai": "^4.3.6",
    "concurrently": "^7.1.0",
    "dotenv": "^16.0.1",
    "mocha": "^10.0.0",
    "supertest": "^6.2.3",
    "ts-node-dev": "^2.0.0-0",
    "vite": "^2.9.9",
    "vue-tsc": "^0.34.13"
  },
  "engines": {
    "node": ">=17.0.0",
    "npm": ">=8.0.0"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      ".mikro-orm.config.ts"
    ]
  }
}
